{
  "manifest_version": 2,
  
  "name": "ClickTweet",
  "description": "Tweet whatever you are viewing (text, images, links etc) with a right-click!",
  "version": "1.1",
  "background": {
    "scripts": ["background.js"]
  },
  "icons" : {
    "64" : "icon2.png",
    "32" : "icon2.png",
    "16" : "icon2.png",
    "128" : "icon2.png"
  },
  "permissions": [
    "contextMenus"
    ]
}
